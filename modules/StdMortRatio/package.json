{
	name:'StdMortRatio',
	group:'Counts',
	title: 'Standardized Mortality Ratio',
	titleShort: 'Std.Mort.Ratio',
	inputFields:
	[
		{
			id: 'observedNumberOfCases',
			label: 'Observed number of cases',
			required: true,
			type: 'text',
			jqmType: 'textinput',
			minlength: 5,
			maxlength: 50,
			defaultValue: '4'
		}
		,{
			id: 'expectedNumberOfCases',
			label: 'Expected number of cases',
			required: true,
			type: 'text',
			jqmType: 'textinput',
			minlength: 5,
			maxlength: 50,
			defaultValue: '3'
		}
	]
	,
	calculate: function(viewModel) { 
		return viewModel.observedNumberOfCases() * viewModel.expectedNumberOfCases();
	}
}
